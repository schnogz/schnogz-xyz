{
  "name": "schnogzdotcom",
  "description": "Andrew Schneider's personal site built with Gatsby",
  "version": "1.0.5",
  "author": "Andrew Schneider <andrew.mark.schneider@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "@loadable/component": "5.14.1",
    "@n8tb1t/use-scroll-position": "2.0.3",
    "framer-motion": "3.2.0",
    "gatsby-plugin-preconnect": "1.1.54",
    "lodash": "4.17.20",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-github-calendar": "2.0.2",
    "react-helmet": "6.1.0",
    "react-tooltip": "4.2.17",
    "styled-components": "5.2.3"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.12.0",
    "gatsby": "2.30.1",
    "gatsby-link": "2.9.0",
    "gatsby-plugin-google-analytics": "2.9.0",
    "gatsby-plugin-react-helmet": "3.8.0",
    "gatsby-plugin-robots-txt": "1.5.5",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-sitemap": "2.10.0",
    "gatsby-plugin-styled-components": "3.8.0",
    "husky": "4.3.7",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1"
  }
}

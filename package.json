{
  "name": "schnogzdotcom",
  "description": "Andrew Schneider's personal site built with Gatsby",
  "version": "1.0.5",
  "author": "Andrew Schneider <andrew.mark.schneider@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "@loadable/component": "5.13.1",
    "@n8tb1t/use-scroll-position": "1.0.43",
    "framer-motion": "2.6.5",
    "gatsby-image": "2.4.16",
    "gatsby-plugin-preconnect": "1.1.54",
    "gatsby-plugin-sharp": "2.6.31",
    "gatsby-source-filesystem": "2.3.27",
    "gatsby-transformer-sharp": "2.5.14",
    "lodash": "4.17.20",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-github-calendar": "2.0.1",
    "react-helmet": "6.1.0",
    "react-tooltip": "4.2.9",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.11.1",
    "gatsby": "2.24.53",
    "gatsby-link": "2.4.13",
    "gatsby-plugin-google-analytics": "2.3.13",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-sitemap": "2.4.12",
    "gatsby-plugin-styled-components": "3.3.10",
    "husky": "4.2.5",
    "lint-staged": "10.2.13",
    "prettier": "2.1.1"
  }
}
